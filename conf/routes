# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#POST    /person                    controllers.PersonController.addPerson

POST    /createSchema               controllers.AppController.createTables

GET    /Timezones                   controllers.TimezoneController.read
POST    /Timezone/createTable       controllers.TimezoneController.createTable
POST    /Timezone            controllers.TimezoneController.create
PUT    /Timezone            controllers.TimezoneController.update
POST    /Timezone/delete(:id)       controllers.TimezoneController.delete(id:String)


